source [file join [file dirname [file dirname [info script]]] gui/mcs4_v1_0.gtcl]

# Definitional proc to organize widgets for parameters.
proc init_gui { IPINST } {
  ipgui::add_param $IPINST -name "Component_Name"
  #Adding Page
  set Page_0 [ipgui::add_page $IPINST -name "Page 0"]
  set C_S_AXI_ADDR_WIDTH [ipgui::add_param $IPINST -name "C_S_AXI_ADDR_WIDTH" -parent ${Page_0}]
  set_property tooltip {Width of S_AXI address bus} ${C_S_AXI_ADDR_WIDTH}
  ipgui::add_param $IPINST -name "C_S_AXI_BASEADDR" -parent ${Page_0}
  ipgui::add_param $IPINST -name "C_S_AXI_HIGHADDR" -parent ${Page_0}
  #Adding Group
  set MCS-4_Customization [ipgui::add_group $IPINST -name "MCS-4 Customization" -parent ${Page_0}]
  set_property tooltip {MCS-4 Customization} ${MCS-4_Customization}
  set NUM_ROMS [ipgui::add_param $IPINST -name "NUM_ROMS" -parent ${MCS-4_Customization}]
  set_property tooltip {Number of 4001 ROMs to instantiate} ${NUM_ROMS}
  set NUM_RAMS [ipgui::add_param $IPINST -name "NUM_RAMS" -parent ${MCS-4_Customization}]
  set_property tooltip {Number of 4002 RAMs} ${NUM_RAMS}
  set NUM_RAM_ROWS [ipgui::add_param $IPINST -name "NUM_RAM_ROWS" -parent ${MCS-4_Customization}]
  set_property tooltip {Number of rows of 4002 RAMs} ${NUM_RAM_ROWS}
  set NUM_RAM_COLS [ipgui::add_param $IPINST -name "NUM_RAM_COLS" -parent ${MCS-4_Customization}]
  set_property tooltip {Number of columns of 4002 RAMs} ${NUM_RAM_COLS}



}

proc update_PARAM_VALUE.NUM_RAMS { PARAM_VALUE.NUM_RAMS PARAM_VALUE.NUM_RAM_ROWS PARAM_VALUE.NUM_RAM_COLS } {
	# Procedure called to update NUM_RAMS when any of the dependent parameters in the arguments change
	
	set NUM_RAMS ${PARAM_VALUE.NUM_RAMS}
	set NUM_RAM_ROWS ${PARAM_VALUE.NUM_RAM_ROWS}
	set NUM_RAM_COLS ${PARAM_VALUE.NUM_RAM_COLS}
	set values(NUM_RAM_ROWS) [get_property value $NUM_RAM_ROWS]
	set values(NUM_RAM_COLS) [get_property value $NUM_RAM_COLS]
	set_property value [gen_USERPARAMETER_NUM_RAMS_VALUE $values(NUM_RAM_ROWS) $values(NUM_RAM_COLS)] $NUM_RAMS
}

